require 'rubygems'

$:.push(File.join(File.dirname(__FILE__), '..', 'lib'))

puts $:.inspect

require 'mof2orientdb'

options = Mof2OrientDB::Options.new

begin
  client = Mof2OrientDB::Client.new options
  client.connect
ensure
  client.disconnect
end
